package codingtest.ct.week01;

public class Ex0108 {

	public static void main(String[] args) {
		
		/*
		 * // int i = 0; // // for (i= 0; i < 100; i++) { // if(i == 50) { // break;
		 * //50을 만나면 반복문 에서 나감 // } // // System.out.println(i); // } //
		 */		
		
		//true 무한 반복
		while(true) {
			for ( int i = 0; i<100; i++) {
				if(i == 50 ) {
					break;
				}
				System.out.println(i);
				
			}
			
			
			
		}
		
	}

}
