package codingtest.ct.week01;

public class Ex0111 {

	public static void main(String[] args) {
		
		
		for(int i = 1; i<= 9; i++) {
			for (int j = 2; j <= 9; j++) {
				System.out.printf("%d * %d = %d",j,i,j*i);
				
				
				//구구단 가로로 찍기 ㄷㄷ
				// 2*1 3*1 4*1 ..... 9*1
				//돌고 다음 2*2 2*3 .. 
				
			}
			System.out.println();
		}

	}

}
