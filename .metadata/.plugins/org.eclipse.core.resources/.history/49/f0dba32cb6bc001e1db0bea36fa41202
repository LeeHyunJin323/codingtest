package codingtest.ct.week01;

public class Ex0106 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int result = 0;
	       for (int i = 1; i <= 10; i++) {
	            if (i % 2 == 0) {
	                continue;
	            }
	            result += i;
	        }
	        System.out.println(result);
	    }
	}
	출처: https://ittrue.tistory.com/114 [IT is True:티스토리]
	}

}
