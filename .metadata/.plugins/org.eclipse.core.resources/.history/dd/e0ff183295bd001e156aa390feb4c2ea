package codingtest.ct.week01;

public class Ex0117 {

	public static void main(String[] args) {
		
		/*
		 * 2차원 배열
		 * 
		 * 
		 * 
		 */
		int[][]  a;
		int b[][];
		int[] c[];
		
		/*
		 * 2차원 배열 초기화
		 */
		
		int d[][] = {{1,2},  // 0,0 = 1 / 0,1 = 2
				     {3,4}}; // 1,0 = 3 / 1,2 = 4
		// 원소 개수가 열 {}안의 {}개수가 행
		int e[][]= new int[][]{{5,6},
			                   {7,8}};
		
		int f[][] = new int[3][2];
		int g[][] = new int [3][]; // 열은 없어도 됨
		  g[0] = new int[5]; 
		  g[1] = new int[3]; 
		  g[2] = new int[1];
		  g[0][0] = 0;
		 
		
		
	}

}
